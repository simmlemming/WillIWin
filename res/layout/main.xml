<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="right"
    android:padding="10dip"
    >
    
    <TextView android:id="@+id/txtHeaderLastResult"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Last result"
				android:gravity="center_horizontal"
				android:textSize="20dip"
				android:background="#25000000"
    />
	<include android:id="@+id/cell1"
				layout="@layout/table_last_result"/>
    <TextView android:id="@+id/txtHeaderNext"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Next"
				android:gravity="center_horizontal"
				android:textSize="20dip"
				android:background="#25000000"
				android:layout_marginTop="10dip"
    />
	<include android:id="@+id/cell2"
				layout="@layout/table_next"/>
    
	<Button android:id="@+id/button1"
			android:text="Go"
			android:layout_width="100dip"
			android:layout_height="wrap_content">
			</Button>
</LinearLayout>
